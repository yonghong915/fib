<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fib.uias.mapper.ErrorCodeMapper">
	<sql id="BASE_COLUMN_LIST">
		pk_id,user_code,real_name,user_desc,user_type,user_status,del_flag
	</sql>

	<!-- 查找用户信息 -->
	<select id="selectList"
		parameterType="com.fib.uias.entity.ErrorCodeEntity"
		resultType="com.fib.uias.entity.ErrorCodeEntity">
		SELECT * FROM sys_error_code
		<where>
			<if test="pkId != null">
				and pk_id = #{pkId}
			</if>
			<if test="errorCode != null">
				and error_code = #{errorCode}
			</if>
		</where>
	</select>

</mapper>