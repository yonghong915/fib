	<manual-mapping>
    <![CDATA[
			import java.util.Map;
			import java.util.ArrayList;
			import java.util.List;
			import java.util.HashMap;
			import com.giantstone.cnaps2.messagebean.recv.req.BEPS_123_Out;
			import com.giantstone.cnaps2.messagebean.recv.req.BEPS_123_OutBeps;

			BEPS_123_Out beps123Out = sourceBean.BEPS_123_Out;

			// NPC处理信息
			List npcList = beps123Out.getBeps();

			Map orderMap = target.get("orderList").get(0);

			for(BEPS_123_OutBeps beps : npcList) {
				String value = beps.getUstrd();
				if(value.contains("H01")) {
					//附言
					orderMap.put("postscript",value.substring(5, value.length()));
				} else if(value.contains("H02")){
					//附言2
					orderMap.put("Unstructured2",value.substring(5, value.length()));
				} else if(value.contains("HA1")){
					//备注
					orderMap.put("Unstructured3",value.substring(5, value.length()));
				} else if(value.contains("HA2")){
					//备注2
					orderMap.put("Unstructured4",value.substring(5, value.length()));
				}
			}

	]]>
	</manual-mapping>

