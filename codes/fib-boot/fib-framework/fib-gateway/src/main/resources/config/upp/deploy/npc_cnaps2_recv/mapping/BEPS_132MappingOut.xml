	<manual-mapping> <![CDATA[
		import java.util.Map;
		import java.util.ArrayList;
		import java.util.List;
		import java.util.HashMap;
		import com.giantstone.cnaps2.messagebean.recv.req.BEPS_132_OutBeps;
		// NPC处理信息
		List ccti = sourceBean.BEPS_132_Out.beps;
		for(BEPS_132_OutBeps beps132Ccti : ccti){
			
			String value = beps132Ccti.AdditionalInformation;
			String name = "";
			if(value.contains("F61")){//包处理状态
				name="processStatus";
			}else if(value.contains("C01")){//NPC轧差日期
				name="nettingDate";	
			}else if(value.contains("E05")){// NPC轧差场次
				name="nettingRound";	
			}else if(value.contains("C00")){//NPC清算日期/终态日期
				name="settlementDate";	
			}
			
			if(!name.equals("")){
				value = value.substring(5);
				target.put(name,value);
			}

		}
		]]>
		</manual-mapping>
