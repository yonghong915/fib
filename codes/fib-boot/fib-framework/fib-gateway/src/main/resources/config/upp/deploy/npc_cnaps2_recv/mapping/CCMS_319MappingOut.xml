	<manual-mapping> 
	<![CDATA[
		import java.util.Map;
		import java.util.ArrayList;
		import java.util.List;
		import java.util.HashMap;
		import com.giantstone.cnaps2.messagebean.recv.req.*;
			
			Map extendMap = target.get("extendMap");
		
			List addInfo = sourceBean.CCMS_319_Out.AdditionalInformation;
		
			List infoList = new ArrayList();
			
			for(CCMS_319_OutAdditionalInformation addInf : addInfo){
				String value = addInf.getAddtlInf();
				if(value.contains("F44")){//退回类型
					extendMap.put("returnType",value.substring(5));
				}else if(value.contains("F45")){//退回业务状态
					extendMap.put("responsionCode",value.substring(5));
				}else if(value.contains("H01")){//备注
					extendMap.put("postscript",value.substring(5));
				}
			}
		]]>			
