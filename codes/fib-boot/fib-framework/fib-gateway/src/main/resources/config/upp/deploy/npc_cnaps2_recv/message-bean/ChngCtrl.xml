<?xml version="1.0" encoding="UTF-8"?>
<message-bean id="ChngCtrl" type="xml" pack-mode="template" short-text="数据变更组件" message-charset="UTF-8" class="com.giantstone.cnaps2.messagebean.recv.req.ChngCtrl">
	<!-- 用于在报文中描述数据变更信息 -->
	<field name="ChangeType" short-text="变更类型" field-type="fixed-field" data-type="str" length="4" required="true" shield="true" xpath="default:ChngTp/text()"/>
	<field name="EffectiveType" short-text="生效类型" field-type="fixed-field" data-type="str" length="4" required="true" shield="true" xpath="default:FctvTp/text()"/>
	<field name="EffectiveDate" short-text="生效日期" field-type="fixed-field" data-type="datetime" pattern="yyyy-mm-dd" length="10" required="false" shield="true" xpath="default:FctvDt/text()"/>
	<field name="IneffectiveDate" short-text="失效日期" field-type="fixed-field" data-type="datetime" pattern="yyyy-mm-dd" length="10" required="false" shield="true" xpath="default:IfctvDt/text()"/>
<template>
<![CDATA[
<ChngTp>${ChangeType}</ChngTp>
<FctvTp>${EffectiveType}</FctvTp>
<FctvDt>${EffectiveDate}</FctvDt>
<IfctvDt>${IneffectiveDate}</IfctvDt>
]]>
</template>
</message-bean>