<?xml version="1.0" encoding="UTF-8"?>
<message-bean id="OrgnlGrpHdr" type="xml" pack-mode="template" short-text="原报文主键组件" message-charset="UTF-8" class="com.giantstone.cnaps2.messagebean.recv.req.OrgnlGrpHdr">
	<!-- 用于在回复报文中标识原报文 -->
	<field name="OriginalMessageIdentification" short-text="原报文标识号" field-type="fixed-field" data-type="str" length="35" xpath="default:OrgnlMsgId/text()"/><!-- 禁止中文，必输，下面两个也一样，其他属性已经配置完成 -->
	<field name="OriginalInstructingParty" short-text="原发起参与机构" field-type="fixed-field" data-type="str" length="14" xpath="default:OrgnlInstgPty/text()"/>
	<field name="OriginalMessageType" short-text="原报文类型" field-type="fixed-field" data-type="str" length="35" xpath="default:OrgnlMT/text()"/>
<template>
<![CDATA[
<OrgnlMsgId>${OriginalMessageIdentification}</OrgnlMsgId>
<OrgnlInstgPty>${OriginalInstructingParty}</OrgnlInstgPty>
<OrgnlMT>${OriginalMessageType}</OrgnlMT>
]]>
</template>
</message-bean>