<?xml version="1.0" encoding="UTF-8"?>
<message-bean id="PBCS_608_Out" type="xml" xpath="." pack-mode="template" message-charset="UTF-8" class="com.giantstone.cnaps2.messagebean.recv.req.PBCS_608_Out" name-space="default=urn:cnaps:std:pbcs:2010:tech:xsd:pbcs.608.001.01|xsi=http://www.w3.org/2001/XMLSchema-instance|schemaLocation=urn:cnaps:std:pbcs:2010:tech:xsd:pbcs.608.001.01 pbcs.608.001.01.xsd">

	<!--2.36	计费与返还清单报文<pbcs.608.001.01>ZOUFAJUAN -->

	<field name="GrpHdr" short-text="业务头" field-type="combine-field" reference="GrpHdr" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:GrpHdr"/>

	<field name="ChargeType" short-text="计费与返还类型" field-type="fixed-field" data-type="str" length="35" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:ChrgTp/text()"/>
	<field name="ChargeParticipant" short-text="计费直接参与者" field-type="fixed-field" data-type="str" length="14" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:ChrgPtcpt/text()"/>
	<field name="ChargeMonth" short-text="计费月份" field-type="fixed-field" data-type="str" length="6" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:ChrgMnth/text()"/>
	<field name="StartDate" short-text="计费开始日期" field-type="fixed-field" data-type="datetime" pattern="yyyy-MM-dd" length="10" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:StartDt/text()"/>
	<field name="EndDate" short-text="计费终止日期" field-type="fixed-field" data-type="datetime" pattern="yyyy-MM-dd" length="10" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:EndDt/text()"/>
	<field name="CurrencyType" short-text="币种" field-type="fixed-field" data-type="str" length="5" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:TtlAmt/@Ccy"/>
	<field name="TotalAmount" short-text="计费/返还总金额" field-type="fixed-field" data-type="str" length="19" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:TtlAmt/text()"/>
	<field name="NumberOfSystem" short-text="计费业务系统数目" field-type="fixed-field" data-type="str" length="35" shield="true" required="true" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf/default:NbOfSys/text()"/>

	<field name="ChargeInformation" short-text="计费清单信息" field-type="table" xpath="default:Document/default:ChrgAndRtrList/default:ChrgAndRtrListInf" row-xpath="default:ChrgInf">
		<field name="ChargeSystem" short-text="计费所属系统" field-type="fixed-field" data-type="str" length="35" shield="true" required="true" xpath="default:ChrgSys/text()"/>
		<field name="CurrencyType2" short-text="币种" field-type="fixed-field" data-type="str" length="5" xpath="default:Amt/@Ccy"/>
		<field name="Amount" short-text="计费/返还总金额" field-type="fixed-field" data-type="str" length="19" shield="true" required="true" xpath="default:Amt/text()"/>
	</field>

	<!--2.36	计费与返还清单报文<pbcs.608.001.01>ZOUFAJUAN-->
<template><![CDATA[
<?xml version="1.0" encoding="UTF-8"?>
<Document xsi:schemaLocation="urn:cnaps:std:pbcs:2010:tech:xsd:pbcs.608.001.01 pbcs.608.001.01.xsd" xmlns="urn:cnaps:std:pbcs:2010:tech:xsd:pbcs.608.001.01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<ChrgAndRtrList>
<GrpHdr>${GrpHdr}</GrpHdr>
<ChrgAndRtrListInf>
<ChrgTp>${ChargeType}</ChrgTp>
<ChrgPtcpt>${ChargeParticipant}</ChrgPtcpt>
<ChrgMnth>${ChargeMonth}</ChrgMnth>
<StartDt>${StartDate}</StartDt>
<EndDt>${EndDate}</EndDt>
<TtlAmt Ccy="${CurrencyType}">${TotalAmount}</TtlAmt>
<NbOfSys>${NumberOfSystem}</NbOfSys>
<#list ChargeInformation#
	<ChrgInf>
		<ChrgSys>${ChargeSystem}</ChrgSys>
		<Amt Ccy="${CurrencyType2}">${Amount}</Amt>
	</ChrgInf>
#>
</ChrgAndRtrListInf>
</ChrgAndRtrList>
</Document>
]]>
</template>
</message-bean>