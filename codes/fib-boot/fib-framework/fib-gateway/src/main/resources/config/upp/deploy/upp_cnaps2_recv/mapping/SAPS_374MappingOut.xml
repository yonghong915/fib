<?xml version="1.0" encoding="UTF-8"?>

<message-bean-mapping id="SAPS_374MappingOut" source-type="map" target-type="message-bean" target-bean-class="com.giantstone.cnaps2.messagebean.send.req.SAPS_374_bean">
  <!-- 报文头 begin -->
  <mapping type="value" to="MsgHeader.VersionID">02</mapping><!--版本 -->
  <mapping type="value-mapping" from="origSender" to="MsgHeader.OrigSender"/><!--报文发起人 -->
  <mapping type="value" to="MsgHeader.OrigSenderSID">SAPS</mapping><!--发送系统号 -->
  <mapping type="value-mapping" from="extendMap.origReceiver" to="MsgHeader.OrigReceiver"/><!--报文接收人 -->
  <mapping type="value" to="MsgHeader.OrigReceiverSID">SAPS</mapping><!--接收系统号 -->
  <mapping type="script" to="MsgHeader.OrigSendDate"> <![CDATA[
	 	 import java.util.Date;
	   	 import java.text.SimpleDateFormat;
	   	 String date =  new SimpleDateFormat("yyyyMMdd").format(new Date());
		 return date;
		]]> </mapping>
  <!--报文发起日期 -->
  <mapping type="script" to="MsgHeader.OrigSendTime"><![CDATA[
	 	 import java.util.Date;
	   	 import java.text.SimpleDateFormat;
	   	 String date =  new SimpleDateFormat("HHmmss").format(new Date());
		 return date;
		]]> </mapping>
  <!--报文发起时间-->
  <mapping type="value" to="MsgHeader.StructType">XML</mapping>
  <!-- 格式类型 -->
  <mapping type="value" to="MsgHeader.MesgType">saps.374.001.01</mapping>
  <!-- 报文类型代码 -->
  <mapping type="value-mapping" from="mesgID" to="MsgHeader.MesgID"/>
  <!-- 通信级标识号 -->
  <mapping type="value-mapping" from="mesgRefID" to="MsgHeader.MesgRefID"/>
  <mapping type="value" to="MsgHeader.MesgPriority">3</mapping>  
  <mapping type="value" to="MsgHeader.MesgDirection">U</mapping>
  <!-- 报文头 end-->
  <!-- 报文体 -->
  <!-- 业务头 -->
  <mapping type="value-mapping" from="messageIdentification" to="SAPS_374_Out.GrpHdr.MessageIdentification"/>
  <!-- 报文标识号 -->
  <mapping type="script" to="SAPS_374_Out.GrpHdr.CreationDateTime"><![CDATA[
		 import java.util.Date;
		 import java.text.SimpleDateFormat;
		 String date = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss").format(new Date());
		 return date;
		]]></mapping>
  <!-- 报文发送时间 -->
  
  <!-- 发起直接参与机构-->
  <mapping type="value-mapping" from="instructingDirectParty" to="SAPS_374_Out.GrpHdr.InstructingDirectParty"/>
  <!--发起参与机构 -->
  <mapping type="value-mapping" from="instructingDirectParty" to="SAPS_374_Out.GrpHdr.InstructingParty"/>
  <!-- 接收直接参与机构 -->
  <mapping type="value-mapping" from="extendMap.origReceiver" to="SAPS_374_Out.GrpHdr.InstructedDirectParty"/>
  <!-- 接收参与机构 -->
  <mapping type="value-mapping" from="extendMap.origReceiver" to="SAPS_374_Out.GrpHdr.InstructedParty"/>
  <!-- 系统编号 -->
  <mapping type="value" to="SAPS_374_Out.GrpHdr.SystemCode">SAPS</mapping>
  <!-- 备注 -->
  <mapping type="value" to="SAPS_374_Out.GrpHdr.Remark">REMARK</mapping>
  
  <!-- 其他 -->
  <!--拆入清算行行号 -->
  <mapping type="value-mapping" from="extendMap.creditor" to="SAPS_374_Out.Creditor"/>
  <!-- 拆出清算行行号 -->
  <mapping type="value-mapping" from="extendMap.debtor" to="SAPS_374_Out.Debtor"/>
  <!--自动拆借管理类型 -->
  <mapping type="value-mapping" from="extendMap.interbankLoanMgmtType" to="SAPS_374_Out.InterbankLoanManagementType"/>
  <!-- 拆借协议号 -->
  <mapping type="value-mapping" from="extendMap.protocolNumber" to="SAPS_374_Out.ProtocolNumber"/>
  <!--币种 -->
  <mapping type="value-mapping" from="extendMap.currencyType" to="SAPS_374_Out.CurrencyType"/>
  <!--拆借金额下限 -->
  <mapping type="value-mapping" from="extendMap.interbankLoanLowerLimit" to="SAPS_374_Out.InterbankLoanLowerLimit"/>
  <!--币种-->
  <mapping type="value-mapping" from="extendMap.currencyType" to="SAPS_374_Out.CurrencyType2"/>
  <!-- 拆借金额上限 -->
  <mapping type="value-mapping" from="extendMap.interbankLoanUpperLimit" to="SAPS_374_Out.InterbankLoanUpperLimit"/>
  <!--币种-->
  <mapping type="value-mapping" from="extendMap.currencyType" to="SAPS_374_Out.CurrencyType3"/>
  <!-- 恢复金额 -->
  <mapping type="value-mapping" from="extendMap.amount" to="SAPS_374_Out.Amount"/>
</message-bean-mapping>
