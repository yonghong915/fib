<?xml version="1.0" encoding="UTF-8"?>
<message-bean id="NPCPrcInf" type="xml" pack-mode="template" short-text="NPC信息组件" message-charset="UTF-8"
	class="com.fib.upp.cnaps2.messagebean.recv.req.NPCPrcInf" xmlns="http://www.fib.com/schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.fib.com/schema 
	                    http://www.fib.com/schema/xsds/message-bean.xsd">

	<!-- 用于应答报文中描述应答状态及相关信息 -->
	<field name="ProcessStatus" short-text="NPC处理状态" field-type="fixed-field" data-type="str" length="4"
		xpath="default:PrcSts/text()" />
	<field name="ProcessCode" short-text="NPC处理码" field-type="fixed-field" data-type="str" length="8"
		xpath="default:PrcCd/text()" />
	<field name="RejectInformation" short-text="NPC拒绝信息" field-type="fixed-field" data-type="str" length="105"
		xpath="default:RjctInf/text()" required="false" />
	<field name="NettingDate" short-text="NPC轧差日期" field-type="fixed-field" data-type="datetime" pattern="yyyy-MM-dd"
		length="10" xpath="default:NetgDt/text()" required="false" />
	<field name="NettingRound" short-text="NPC轧差场次" field-type="fixed-field" data-type="str" length="2"
		xpath="default:NetgRnd/text()" required="false" />
	<field name="SettlementDate" short-text="NPC清算日期/终态日期" field-type="fixed-field" data-type="datetime"
		pattern="yyyy-MM-dd" length="10" xpath="default:SttlmDt/text()" required="false" />
	<field name="ReceiveTime" short-text="NPC接收时间" field-type="fixed-field" data-type="datetime" length="19"
		pattern="yyyy-MM-dd'T'HH:mm:ss" xpath="default:RcvTm/text()" required="false" />
	<field name="TransmitTime" short-text="NPC转发时间" field-type="fixed-field" data-type="datetime" length="19"
		pattern="yyyy-MM-dd'T'HH:mm:ss" xpath="default:TrnsmtTm/text()" required="false" />
	<template>
		<![CDATA[
				<PrcSts>${ProcessStatus}</PrcSts>
				<PrcCd>${ProcessCode}</PrcCd>
				<RjctInf>${RejectInformation}</RjctInf>
				<NetgDt>${NettingDate}</NetgDt>
				<NetgRnd>${NettingRound}</NetgRnd>
				<SttlmDt>${SettlementDate}</SttlmDt>
				<RcvTm>${ReceiveTime}</RcvTm>
				<TrnsmtTm>${TransmitTime}</TrnsmtTm>
		]]>
	</template>
</message-bean>